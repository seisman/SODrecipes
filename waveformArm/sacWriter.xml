<waveformArm>
    <distanceRange>
        <unit>DEGREE</unit>
        <max>110</max>
    </distanceRange>
    <phaseRequest>
        <model>prem</model>
        <beginPhase>ttp</beginPhase>
        <beginOffset>
            <unit>SECOND</unit>
            <value>-300</value>
        </beginOffset>
        <endPhase>PKiKP</endPhase>
        <endOffset>
            <unit>SECOND</unit>
            <value>300</value>
        </endOffset>
    </phaseRequest>
    <fdsnDataSelect/>
    <someCoverage/>
    <someDataCoverage/>
    <printlineSeismogramProcess/>
    <collapseOverlaps/>
    <gapFill><zeroFill/></gapFill>
    <merge/>
    <rMean/>
    <rTrend/>
    <taper/>
    <sacWriter/>

    <transferResponse>
        <lowCut>0.01</lowCut>
        <lowPass>0.05</lowPass>
        <highPass>20</highPass>
        <highCut>40</highCut>
    </transferResponse>
    <sacWriter>
        <workingDir>processedSeismograms</workingDir>
        <phaseTimeHeader>
            <model>prem</model>
            <phaseName>P</phaseName>
            <tHeader>0</tHeader>
            <arrivalIndex>1</arrivalIndex>
        </phaseTimeHeader>
        <phaseTimeHeader>
            <model>prem</model>
            <phaseName>PcP</phaseName>
            <tHeader>1</tHeader>
            <arrivalIndex>1</arrivalIndex>
        </phaseTimeHeader>
        <phaseTimeHeader>
            <model>prem</model>
            <phaseName>PKiKP</phaseName>
            <tHeader>2</tHeader>
            <arrivalIndex>1</arrivalIndex>
        </phaseTimeHeader>
    </sacWriter>
    <legacyExecute>
        <command>echo Sod saved this file</command>
    </legacyExecute>
</waveformArm>
